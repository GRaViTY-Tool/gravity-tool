#using MoDiscoTGG.*
#using AttrCondDefLibrary.*

#abstract #rule TypeAccessType2ParameterType_Abstract #with MoDiscoTGG

#source { 
	typeAccess:TypeAccess {
		++-type->jType
	}
	
	jType:Type
}

#target {
	operation:Operation{
		-ownedParameter -> param
	}
	
	param : Parameter{
		++-type->uType
	}
	
	uType:Type
}

#correspondence {
	ta2p:TypeAccess2Parameter {
		#src->typeAccess
		#trg->param
	}
	
	ne2ne:NamedElement2NamedElement {
		#src->jType
		#trg->uType
	}
}

#attributeConditions {
	
}
