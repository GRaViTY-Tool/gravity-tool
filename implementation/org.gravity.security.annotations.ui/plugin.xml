<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.jdt.core.compilationParticipant">
      <compilationParticipant
            class="org.gravity.security.annotations.check.SecureDependencyCheck"
            createsProblems="true"
            id="org.gravity.security.annotations.check.SecureDependencyCheck"
            modifiesEnvironment="false">
      </compilationParticipant>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.gravity.mainmenu">
        <command
                  commandId="security.annotations.ui.add"
                  label="Add Security Annotations Library"
                  style="push">
        </command>
      </menuContribution>
   </extension>
    <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="security.annotations.ui.SecurityAnnotationsAdditionHandler"
            id="security.annotations.ui.add"
            name="Add Security Annotations Library">
         <defaultHandler
               class="security.annotations.ui.SecurityAnnotationsAdditionHandler">
         </defaultHandler>
      </command>
    </extension>
</plugin>
